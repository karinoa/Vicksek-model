import numpy as np

n = 100
mean = 1
sigma = 0.1
particles = np.array(n,dtype=None)

def generate_particles(n):
    return np.random.normal(mean,sigma,n)

    
particles = generate_particles(n)
    
print(generate_particles(n))
print(particles.max())
print(particles.min())



